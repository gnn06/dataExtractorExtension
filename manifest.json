{
    "name": "data extractor",
    "version": "0.1",
    "manifest_version": 2,

    "description": "web data extraction",
    "icons": {
		"128": "icon_128.png"
    },
    "browser_action": {
		"default_title": "",
		"default_icon": "icon_19.png"
    },

    "background": {
        "scripts": [
            "js/contentscript/background.js"
        ]
    },

    "permissions": [
		"storage", "tabs", "activeTab", "http://*/*",
		"https://*/*"
    ],

    "content_security_policy": "script-src 'self' blob: filesystem: chrome-extension-resource: 'unsafe-eval'; ; object-src 'self'"
}
